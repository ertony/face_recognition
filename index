    public void close(){
    WindowEvent winClosingEvent= new WindowEvent(null,WindowEvent.WINDOW_CLOSING);
            Toolkit.getDefaultToolkit().getSystemEventQueue().postEvent(winClosingEvent);}
 
 
    private void capturerActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // TODO add your handling code here:

        mythread.runnable=false;
        capturer.setEnabled(false);
        startC.setEnabled(true);

        websource.release();

    }                                        

    private void startCActionPerformed(java.awt.event.ActionEvent evt) {                                       
        websource= new VideoCapture(0);
        mythread= new DaemonThread();
        Thread t=new Thread(mythread);
        t.setDaemon(true);
        mythread.runnable=true;
        t.start();
        startC.setEnabled(false);
        capturer.setEnabled(true);

        // TODO add your handling code here:
    }                                      

    private void ajouterActionPerformed(java.awt.event.ActionEvent evt) {                                        
int returnval =jFilechooser1.showSaveDialog(this);
if(returnval==jFilechooser1.APPROVE_OPTION)   {
File file=jFilechooser1.getSelectedFile();
Highgui.imwrite(file.getPath(), frame) ;
}else{
System.out.println("Acce annuler");
}        // TODO add your handling code here:
    }                                       

    private void jMenuItem4ActionPerformed(java.awt.event.ActionEvent evt) {                                           
        // TODO add your handling code here:
    }                                          

    private void fermerActionPerformed(java.awt.event.ActionEvent evt) {                                       
        // TODO add your handling code here:
        System.exit(0);
        close();
    }                                      

    private void jMenuItem3ActionPerformed(java.awt.event.ActionEvent evt) {                                           
        // TODO add your handling code here:
        System.exit(0);
        close();
    }                                          

